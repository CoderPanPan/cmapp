    <declare-styleable name="LineWaveVoiceView">
        <attr format="color" name="voiceLineColor"/>
        <attr format="dimension" name="voiceLineWidth"/>
        <attr format="color" name="voiceTextColor"/>
        <attr format="dimension" name="voiceTextSize"/>
    </declare-styleable>
    <declare-styleable name="PasswordInput">
        <!--获得焦点时的颜色-->
        <attr format="color" name="focusedColor"/>
        <!--未获得焦点时的颜色-->
        <attr format="color" name="notFocusedColor"/>
        <!--背景色-->
        <attr format="color" name="backgroundColor"/>
        <!--字符方块的数量-->
        <attr format="integer" name="boxCount"/>
        <!--获得焦点时颜色是否改变-->
        <attr format="boolean" name="focusColorChangeEnable"/>
        <!--圆点半径-->
        <attr format="dimension" name="dotRaduis"/>
    </declare-styleable>
    <declare-styleable name="SwitchButton">

        <attr format="string" name="enterTitle"/>
        <attr format="string" name="cancelTitle"/>
        <attr format="reference" name="enterSrc"/>
        <attr format="reference" name="cancelSrc"/>
        <attr format="integer" name="interval"/>

    </declare-styleable>
    <declare-styleable name="WaveProgressView">
        <attr format="integer" name="progress_max"/>
        <attr format="integer" name="progress"/>
        <attr format="float" name="speed"/>
        <attr format="float" name="wave_width"/>
        <attr format="float" name="wave_height"/>
        <attr format="color" name="wave_color"/>
        <attr format="color" name="wave_bg_color"/>
        <attr format="color" name="stroke_color"/>
        <attr format="string" name="main_text"/>
        <attr format="color" name="main_text_color"/>
        <attr format="dimension" name="main_text_size"/>
        <attr format="string" name="hint_text"/>
        <attr format="color" name="hint_color"/>
        <attr format="dimension" name="hint_size"/>
        <attr format="dimension" name="text_space"/>
    </declare-styleable>
    <declare-styleable name="voiceView">
        <!--中间线的颜色，就是波形的时候，大家可以看到，中间有一条直线，就是那个-->
        <attr format="color" name="middleLine"/>
        <!--中间线的高度，因为宽度是充满的-->
        <attr format="dimension" name="middleLineHeight"/>
        <!--波动的线的颜色，如果是矩形样式的话，刚是矩形的颜色-->
        <attr format="color" name="voiceLine"/>
        <!--波动线的横向移动速度，线的速度的反比，即这个值越小，线横向移动越快，越大线移动越慢，默认90-->
        <attr format="integer" name="lineSpeed"/>
        <!--矩形的宽度-->
        <attr format="dimension" name="rectWidth"/>
        <!--矩形之间的间隔-->
        <attr format="dimension" name="rectSpace"/>
        <!--矩形的初始高度，就是没有声音的时候，矩形的高度-->
        <attr format="dimension" name="rectInitHeight"/>
        <!--所输入音量的最大值-->
        <attr format="float" name="maxVolume"/>
        <!--控件的样式，一共有两种，波形或者矩形-->
        <attr name="viewMode">
            <enum name="line" value="0"/>
            <enum name="rect" value="1"/>
        </attr>
        <!--灵敏度，默认值是4-->
        <attr name="sensibility">
            <enum name="one" value="1"/>
            <enum name="two" value="2"/>
            <enum name="three" value="3"/>
            <enum name="four" value="4"/>
            <enum name="five" value="5"/>
        </attr>
        <!--精细度，绘制曲线的时候，每几个像素绘制一次，默认是1，一般，这个值越小，曲线越顺滑，
            但在一些旧手机上，会出现帧率过低的情况，可以把这个值调大一点，在图片的顺滑度与帧率之间做一个取舍-->
        <attr name="fineness">
            <enum name="one" value="1"/>
            <enum name="two" value="2"/>
            <enum name="three" value="3"/>
        </attr>
    </declare-styleable>
